{
    "dialog": {
        "version": 1,
        "type": "basic",
        "context": {
            "version": 1,
            "size": 1024,
            "n-vocab": 32000,
            "bos-token": 1,
            "eos-token": 2
        },
        "sampler": {
            "version": 1,
            "seed": 42,
            "temp": 0.8,
            "top-k": 40,
            "top-p": 0.95
        },
        "tokenizer": {
            "version": 1,
            "path": "<tokenizer_path>"
        },
        "engine": {
            "version": 1,
            "n-threads": 4,
            "backend": {
                "version": 1,
                "type": "QnnHtp",
                "QnnHtp": {
                    "version": 1,
                    "spill-fill-bufsize": 431294976,
                    "use-mmap": false,
                    "mmap-budget": 0,
                    "poll": true,
                    "pos-id-dim": 64,
                    "cpu-mask": "0xe0",
                    "kv-dim": 128
                },
                "extensions": "<htp_backend_ext_path>"
            },
            "model": {
                "version": 1,
                "type": "binary",
                "binary": {
                    "version": 1,
                    "ctx-bins": [
                        "<models_path>\\llama2_0.serialized.bin",
                        "<models_path>\\llama2_1.serialized.bin",
                        "<models_path>\\llama2_2.serialized.bin",
                        "<models_path>\\llama2_3.serialized.bin"
                    ]
                }
            }
        }
    }
}
